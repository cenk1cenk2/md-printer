{
  "name": "@cenk1cenk2/md-printer",
  "version": "2.2.1",
  "description": "A markdown printer.",
  "main": "dist/index.js",
  "repository": "git@gitlab.kilic.dev:utils/md-printer.git",
  "author": "Cenk Kilic",
  "license": "MIT",
  "bin": {
    "md-printer": "bin/run.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "./bin/run.js",
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --write src/ --loglevel warn && eslint --ext .json,.ts,.js --fix src/",
    "lint:check": "eslint --ext .json,.ts,.js src/",
    "lint:staged": "lint-staged",
    "readme": "yarn oclif-dev readme",
    "oclif": "oclif",
    "oclif-dev": "node -r tsconfig-paths/register node_modules/@oclif/dev-cli/bin/run",
    "test": "./bin/run.js ./test/test.md --debug",
    "template:privat-rechnung": "cd ./templates/privat-rechnung && tailwindcss -w -c ./tailwind.config.js -i ./tailwind.css -o ./main.css"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn exec lint-staged",
    "prepare-commit-msg": "[ -t 1 ] && exec < /dev/tty && git cz --hook || true"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "files": [
    "bin/",
    "config/",
    "dist/",
    "templates/"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "bin": "md-printer",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./dist/hooks/init.hook",
      "command_not_found": "./dist/hooks/not-found.hook",
      "prerun": "./dist/hooks/prerun.hook"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "oclif",
    "cenk1cenk2"
  ],
  "dependencies": {
    "@cenk1cenk2/boilerplate-oclif": "^1.10.6",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^5.1.12",
    "chokidar": "^3.5.3",
    "config": "^3.3.7",
    "enquirer": "^2.3.6",
    "execa": "^5.1.1",
    "fs-extra": "^10.1.0",
    "globby": "^11.0.4",
    "gray-matter": "^4.0.3",
    "listr2": "^4.0.5",
    "md-to-pdf": "^5.1.0",
    "nunjucks": "^3.2.3",
    "tsup": "^6.1.2",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^1.5.3",
    "@cenk1cenk2/eslint-config": "^2.5.16",
    "@oclif/dev-cli": "^1.26.10",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/config": "^0.0.41",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.0",
    "@types/nunjucks": "^3.2.1",
    "eslint": "^8.18.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.0",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.1.4",
    "theme-colors": "^0.0.5",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@cenk1cenk2/cz-cc"
    }
  }
}
