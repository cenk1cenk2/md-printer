{
  "name": "@cenk1cenk2/md-printer",
  "version": "2.3.2",
  "description": "A markdown printer.",
  "main": "dist/index.js",
  "type": "module",
  "repository": "https://gitlab.kilic.dev/utils/md-printer",
  "author": "Cenk Kilic",
  "license": "MIT",
  "bin": {
    "md-printer": "bin/run.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "./bin/dev.js",
    "start": "./bin/run.js",
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "clean": "rimraf oclif.manifset.json",
    "format": "prettier --write src/ --log-level warn && eslint --fix src/",
    "lint": "eslint src/",
    "manifest": "oclif manifest",
    "docs:toc": "oclif readme"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "prepare-commit-msg": "[ -t 1 ] && exec < /dev/tty && git cz --hook || true"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --log-level warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --log-level warn --write"
    ]
  },
  "files": [
    "/config",
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "templates/"
  ],
  "oclif": {
    "bin": "md-printer",
    "dirname": "md-printer",
    "commands": {
      "strategy": "single",
      "target": "./dist/commands/index.js"
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./dist/hooks/update-notifier.hook"
      ],
      "command_not_found": "./dist/hooks/not-found.hook"
    },
    "topicSeparator": " "
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "oclif",
    "cenk1cenk2"
  ],
  "dependencies": {
    "@cenk1cenk2/oclif-common": "^6.3.29",
    "@listr2/manager": "^2.0.13",
    "@oclif/core": "^4.2.10",
    "@oclif/plugin-help": "^6.2.26",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "chokidar": "^4.0.3",
    "fs-extra": "^11.3.0",
    "gray-matter": "^4.0.3",
    "listr2": "^8.2.5",
    "md-to-pdf": "^5.2.4",
    "nunjucks": "^3.2.4",
    "postcss": "^8.5.3",
    "showdown": "^2.1.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^1.8.0",
    "@cenk1cenk2/eslint-config": "^3.1.27",
    "@swc/core": "^1.10.16",
    "@types/config": "^3.3.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.13",
    "@types/nunjucks": "^3.2.6",
    "@types/showdown": "^2.0.6",
    "eslint": "^9.20.1",
    "execa": "^9.5.2",
    "globby": "^14.1.0",
    "lint-staged": "^15.5.0",
    "oclif": "^4.17.41",
    "prettier": "^3.5.1",
    "simple-git-hooks": "^2.12.1",
    "source-map-support": "^0.5.21",
    "theme-colors": "^0.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-replace-paths": "^0.0.14",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@cenk1cenk2/cz-cc"
    }
  },
  "packageManager": "pnpm@9.6.0"
}
