{
  "name": "@cenk1cenk2/md-printer",
  "version": "2.2.75",
  "description": "A markdown printer.",
  "main": "dist/index.js",
  "type": "module",
  "repository": "https://gitlab.kilic.dev/utils/md-printer",
  "author": "Cenk Kilic",
  "license": "MIT",
  "bin": {
    "md-printer": "bin/run.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "./bin/dev.js",
    "start": "./bin/run.js",
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "clean": "rimraf oclif.manifset.json",
    "format": "prettier --write src/ --loglevel warn && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "manifest": "oclif manifest",
    "docs:toc": "oclif readme"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "prepare-commit-msg": "[ -t 1 ] && exec < /dev/tty && git cz --hook || true"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "files": [
    "/config",
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "templates/"
  ],
  "oclif": {
    "bin": "md-printer",
    "dirname": "md-printer",
    "commands": {
      "strategy": "single",
      "target": "./dist/commands/index.js"
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./dist/hooks/update-notifier.hook"
      ],
      "command_not_found": "./dist/hooks/not-found.hook"
    },
    "topicSeparator": " "
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "oclif",
    "cenk1cenk2"
  ],
  "dependencies": {
    "@cenk1cenk2/oclif-common": "^6.3.14",
    "@listr2/manager": "^2.0.8",
    "@oclif/core": "^3.26.6",
    "@oclif/plugin-help": "^6.1.0",
    "chokidar": "^3.6.0",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "listr2": "^8.2.1",
    "md-to-pdf": "^5.2.4",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^1.7.7",
    "@cenk1cenk2/eslint-config": "^2.7.48",
    "@swc/core": "^1.5.24",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/config": "^3.3.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.13.0",
    "@types/nunjucks": "^3.2.6",
    "eslint": "^8.57.0",
    "execa": "^9.1.0",
    "globby": "^14.0.1",
    "lint-staged": "^15.2.5",
    "oclif": "^4.12.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "simple-git-hooks": "^2.11.1",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.4.3",
    "theme-colors": "^0.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-replace-paths": "^0.0.14",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@cenk1cenk2/cz-cc"
    }
  }
}
