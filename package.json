{
  "name": "@cenk1cenk2/md-printer",
  "version": "2.2.35",
  "description": "A markdown printer.",
  "main": "dist/index.js",
  "repository": "https://gitlab.kilic.dev/utils/md-printer",
  "author": "Cenk Kilic",
  "license": "MIT",
  "bin": {
    "md-printer": "bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "./bin/dev",
    "start": "./bin/run",
    "build": "tsup-node",
    "dev:start": "tsup-node --watch",
    "clean": "rimraf oclif.manifset.json",
    "format": "prettier --write src/ --loglevel warn && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "manifest": "oclif manifest",
    "docs:toc": "oclif readme"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "prepare-commit-msg": "[ -t 1 ] && exec < /dev/tty && git cz --hook || true"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "files": [
    "/config",
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "templates/"
  ],
  "oclif": {
    "bin": "md-printer",
    "dirname": "md-printer",
    "commands": "./dist/commands",
    "default": ".",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./dist/hooks/update-notifier.hook"
      ],
      "command_not_found": "./dist/hooks/not-found.hook"
    },
    "topicSeparator": ":"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "oclif",
    "cenk1cenk2"
  ],
  "dependencies": {
    "@cenk1cenk2/oclif-common": "^5.0.5",
    "@oclif/core": "^2.8.5",
    "@oclif/plugin-help": "^5.2.9",
    "chokidar": "^3.5.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "enquirer": "^2.3.6",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "globby": "^11.1.0",
    "gray-matter": "^4.0.3",
    "listr2": "^6.5.0",
    "md-to-pdf": "^5.2.4",
    "nunjucks": "^3.2.4",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@cenk1cenk2/cz-cc": "^1.6.5",
    "@cenk1cenk2/eslint-config": "^2.7.4",
    "@swc/core": "^1.3.57",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/config": "^3.3.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.5",
    "@types/nunjucks": "^3.2.2",
    "eslint": "^8.40.0",
    "lint-staged": "^13.2.2",
    "oclif": "^3.9.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.3.2",
    "theme-colors": "^0.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-replace-paths": "^0.0.14",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@cenk1cenk2/cz-cc"
    }
  }
}
